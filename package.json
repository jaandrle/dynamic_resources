{
    "name": "dynamic_resources",
    "version": "1.0.0",
    "description": "Yet another lightweight promise-based scripts/stylesheets loader (no support for IE).",
    "main": "gulpfile.js",
    "sequence": [
        "javascript",
        "doc",
        "examples"
    ],
    "app_keys_map": [
        [
            "name"
        ],
        [
            "version"
        ],
        [
            "homepage"
        ],
        [
            "directories"
        ],
        [
            "sequence"
        ]
    ],
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "check": "node ./jaaJBT.js check && npm outdated",
        "update": "node ./jaaJBT.js update && npm install",
        "jshint": "node ./node_modules/jshint/bin/jshint --verbose",
        "jshint_full": "node ./node_modules/jshint/bin/jshint --verbose --show-non-errors"
    },
    "directories": {
        "gulp": "gulp/",
        "src": "src/",
        "bin": "bin/",
        "doc": "docs/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jaandrle/dynamic_resources.git"
    },
    "author": "Jan Andrle <andrle.jan@centrum.cz>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/jaandrle/dynamic_resources/issues"
    },
    "homepage": "https://github.com/jaandrle/dynamic_resources#readme",
    "devDependencies": {
        "gulp": "^4.0.2",
        "gulp-minify": "^3.1.0",
        "gulp-place": "^1.8.1",
        "gulp-rename": "^2.0.0",
        "gulp-replace": "^1.0.0",
        "gulp-util": "^3.0.8",
        "jsdoc-to-markdown": "^6.0.1",
        "jshint": "^2.12.0"
    },
    "jaaJBT": {
        "config": {
            "gulp_path": "gulp/",
            "gulp_tasks": "gulp/tasks/",
            "jsdoc2md_modifications_path": "src_docs/modifications/",
            "doc_path": "docs/",
            "examples_nth_path": "src_docs/examples/",
            "examples_main_path": "src_docs/"
        },
        "scripts": {
            "gulpfile": "~1.1.4",
            "gulp_cleanJSHINT": "~0.1.3",
            "gulp_task_javascript_lib": "~0.1.5",
            "gulp_task_default": "~1.0.1",
            "gulp_place_type": "~1.8.1",
            "jsconfig": "~1.0.0",
            "jsdoc2md__libs_gulp_task": "~1.0.0",
            "jsdoc2md__libs_modifications": "~1.0.0",
            "examples__libs_gulp_task": "~1.0.1",
            "examples__libs_css": "~1.0.0",
            "examples__libs_nth": "~1.0.0",
            "examples__libs_main": "~1.0.0"
        },
        "resourses": [
            "https://raw.githubusercontent.com/jaandrle/jaaJBT/master/",
            "https://raw.githubusercontent.com/jaandrle/jsdoc2md-jsonDoc--partial-helpers/main/"
        ],
        "rename": {}
    },
    "jshintConfig": {
        "esversion": 6,
        "browser": "true",
        "undef": "true",
        "latedef": "true",
        "maxparams": 4,
        "maxdepth": 2,
        "maxcomplexity": 10,
        "globals": {
            "gulp_place": false
        }
    }
}